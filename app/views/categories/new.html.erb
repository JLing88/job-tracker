<h1>Create New Category</h1><br />

<%= form_for(@category) do |f| %>
  <ul>
    <% @category.errors.full_messages.each do |error| %>
      <li> <%= error %> </li>
    <% end %>
  </ul>
  <br />
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
